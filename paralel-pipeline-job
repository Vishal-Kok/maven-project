pipeline
{
	agent any
	stages
	{
		stage('scm')
		{
			steps
			{
				git branch:'master', url:'https://github.com/santoshdeshmane06/maven-project.git'
			}	
		}
		stage('prallel job')
		{
		parallel 
		{
		
		stage('compile')
		{
			steps
			{
				withMaven(maven : 'localmaven') 
                		{   
                    			sh 'mvn compile' 
               			 }
			}
		}
		stage('Package')
		{
			steps
			{
				withMaven(maven : 'localmaven')
               			 {
                 			   sh 'mvn package'
               			 }
			}
		}
		}
		failfast true
		}
	
	}
}
