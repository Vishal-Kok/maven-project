pipeline
{
	agent any
	stages
	{
		stage('scm')
		{
			steps
			{
				git branch:'master', url:'https://github.com/santoshdeshmane06/maven-project.git'
			}	
		}
		stage('prallel job')
		{
		parallel 
		{
		
		stage('compile')
		{
			steps
			{
				withMaven(maven : 'LocalMaven') 
                {   
                    sh 'mvn compile' 
                }
			}
		}
		stage('Package')
		{
			steps
			{
				withMaven(maven : 'LocalMaven')
                {
                    sh 'mvn package'
                }
			}
		}
		}
		}
	}
}
